{
    "tabWidth": 2,
    "bracketSpacing": false,
    "singleQuote": true,
    "bracketSameLine": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "semi": false,
    "overrides": [
      {
        "files": "*.css",
        "options": {
          "parser": "css"
        }
      },
      {
        "files": "*.md",
        "options": {
          "parser": "mdx"
        }
      }
    ]
  }